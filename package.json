{
  "name": "try_type_orm",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/node": "^8.0.29",
    "ts-node": "^3.3.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.29"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
